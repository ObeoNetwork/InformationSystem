<plugin>
   <extension-point id="org.obeonetwork.tools.snapshot.viewer.postImport" name="postImport" schema="schema/postImport.exsd"/>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="org.eclipse.sirius.category"
            class="org.obeonetwork.tools.snapshot.viewer.wizards.ImportModelFromXmlWizard"
