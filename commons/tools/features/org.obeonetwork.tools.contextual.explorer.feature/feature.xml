<feature
      id="org.obeonetwork.tools.contextual.explorer.feature"
      label="Obeo Network - Contextual Explorer Feature"
      version="2.0.0.qualifier"
      provider-name="%providerName"
      license-feature="org.obeonetwork.is.license"
      license-feature-version="2.0.0.qualifier">

   <description url="https://www.obeo.fr">
      Contextual explorer feature
   </description>

   <copyright url="https://www.obeo.fr">
      %copyright
   </copyright>

   <requires>
      <import feature="org.eclipse.amalgam.explorer.contextual" version="1.10.1" match="greaterOrEqual"/>
      <import feature="org.eclipse.amalgam.explorer.contextual.core" version="1.10.1" match="greaterOrEqual"/>
      <import feature="org.eclipse.amalgam.explorer.contextual.sirius" version="1.10.1" match="greaterOrEqual"/>
      <import feature="org.eclipse.amalgam.discovery" version="1.10.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.obeonetwork.tools.contextual.explorer.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.obeonetwork.tools.contextual.explorer.ui.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
