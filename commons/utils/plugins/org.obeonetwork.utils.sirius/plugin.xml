<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.obeonetwork.utils.sirius.propertyTesters.SiriusEditPartPropertyTester"
            id="org.obeonetwork.utils.sirius.propertyTesters.SiriusEditPartPropertyTester"
            namespace="org.eclipse.sirius.diagram.ui.edit.api.part.ISiriusEditPart"
            properties="diagramName,viewpointId,mappingName"
            type="org.eclipse.sirius.diagram.ui.edit.api.part.ISiriusEditPart">
      </propertyTester>
      <propertyTester
            class="org.obeonetwork.utils.sirius.propertyTesters.SiriusTablePropertyTester"
            id="org.obeonetwork.utils.sirius.propertyTesters.SiriusTablePropertyTester"
            namespace="org.eclipse.sirius.table"
            properties="tableName"
            type="org.eclipse.sirius.viewpoint.DRepresentationElement">
      </propertyTester>
      <propertyTester
            class="org.obeonetwork.utils.sirius.propertyTesters.DSemanticDecoratorPropertyTester"
            id="org.obeonetwork.utils.sirius.propertyTesters.DSemanticDecoratorPropertyTester"
            namespace="org.eclipse.sirius.semanticDecorator"
            properties="targetType"
            type="org.eclipse.sirius.viewpoint.DSemanticDecorator">
      </propertyTester>
   </extension>

</plugin>
