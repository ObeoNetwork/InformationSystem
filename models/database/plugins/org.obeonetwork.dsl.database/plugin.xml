<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.obeonetwork.org/dsl/database/1.0"
            class="org.obeonetwork.dsl.database.DatabasePackage"
            genModel="model/database.genmodel"/>
   </extension>

  <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
         type="database"
         class="org.obeonetwork.dsl.database.util.DatabaseResourceFactoryImpl">
      </parser>
   </extension>
  <extension
        point="org.eclipse.core.contenttype.contentTypes">
     <file-association
           content-type="org.eclipse.emf.compare.content.type"
           file-extensions="database"
           file-names="*">
     </file-association>
  </extension>


</plugin>
