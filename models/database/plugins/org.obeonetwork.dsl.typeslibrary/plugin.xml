<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.obeonetwork.org/dsl/typeslibrary/1.0"
            class="org.obeonetwork.dsl.typeslibrary.TypesLibraryPackage"
            genModel="model/typeslibrary.genmodel"/>
   </extension>
   
   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
         type="typeslibrary"
         class="org.obeonetwork.dsl.typeslibrary.util.TypesLibraryResourceFactoryImpl">
      </parser>
   </extension>
   
	<extension point="org.eclipse.emf.ecore.uri_mapping">
		<mapping 
    		source="pathmap://LogicalDBTypes"
    		target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/LogicalModel.typeslibrary">
  		</mapping>
		<mapping
        source="pathmap://NativeDBTypes/MySQL-5"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/MySQL-5.typeslibrary">
  </mapping>
		<mapping
			source="pathmap://NativeDBTypes/Oracle-11g"
		    target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/Oracle-11g.typeslibrary">
		</mapping>
  <mapping
        source="pathmap://NativeDBTypes/H2-1.3"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/H2-1.3.typeslibrary">
  </mapping>
  <mapping
        source="pathmap://NativeDBTypes/SQLServer-2008"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/SQLServer-2008.typeslibrary">
  </mapping>
  <mapping
        source="pathmap://NativeDBTypes/MariaDB-10.2"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/MariaDB-10.2.typeslibrary">
  </mapping>
  <mapping
        source="pathmap://NativeDBTypes/Postgres-9"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/Postgres-9.typeslibrary">
  </mapping>
  <mapping
        source="pathmap://NativeDBTypes/Postgres-9.5-13"
        target="platform:/plugin/org.obeonetwork.dsl.typeslibrary/resources/Postgres-9.5-13.typeslibrary">
  </mapping>
	</extension>
 <extension
       point="org.eclipse.core.contenttype.contentTypes">
    <file-association
          content-type="org.eclipse.emf.compare.content.type"
          file-extensions="typeslibrary"
          file-names="*">
    </file-association>
 </extension>

</plugin>
