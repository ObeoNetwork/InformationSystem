      license-feature="org.obeonetwork.is.license"
      license-feature-version="1.0.0.qualifier">

   <description url="https://www.obeo.fr">
      ObeoNetwork - Environment Designer
   </description>

   <copyright url="https://www.obeo.fr">
      %copyright
   </copyright>

   <requires>
      <import feature="org.eclipse.nebula.treemapper.feature" version="0.1.0" match="greaterOrEqual"/>
      <import feature="org.obeonetwork.dsl.feature.soa" version="2.4.0" match="greaterOrEqual"/>
      <import feature="org.obeonetwork.dsl.feature.entity" version="2.4.0" match="greaterOrEqual"/>
      <import feature="org.obeonetwork.dsl.feature.environment" version="2.4.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.obeonetwork.dsl.environment.design"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.obeonetwork.dsl.environment.properties"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
