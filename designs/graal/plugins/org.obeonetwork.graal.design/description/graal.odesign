                      </cases>
                      <default>
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self"/>
                      </default>
                    </subModelOperations>
                  </default>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
