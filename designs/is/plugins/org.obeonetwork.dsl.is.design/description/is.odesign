        <toolSections name="Binding">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="New binding" nodeMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']">
            <variable name="container">
              <subVariables xsi:type="tool:SelectModelElementVariable" name="left" candidatesExpression="&lt;%eAllContents()%>" tree="true" rootExpression="&lt;%ownedCategories + types%>" childrenExpression="&lt;%types%>" message="Choose the first bound element (must be a DTO)"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="&lt;%$left.getAllBindableElements() - $container.getBoundElementsWithTarget($left)%>"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="right" candidatesExpression="&lt;%($bindableElements + $bindableElements.ancestor()).nMinimize()%>" tree="true" rootExpression="&lt;%allRoots()%>" childrenExpression="&lt;%eContents()%>" message="Choose the second bound element (must be a DTO or Entity)"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$container.getGlobalBindingRegistry()%>">
                <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%$left.filter(&quot;DTO&quot;) != null &amp;&amp; ($right.filter(&quot;DTO&quot;) != null || $right.filter(&quot;Entity&quot;) != null)%>">
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="environment.BindingInfo" referenceName="bindingInfos">
                    <subModelOperations xsi:type="tool:SetValue" featureName="left" valueExpression="&lt;%$left%>"/>
                    <subModelOperations xsi:type="tool:SetValue" featureName="right" valueExpression="&lt;%$right%>"/>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%$left.filter(&quot;DTO&quot;) == null || ($right.filter(&quot;DTO&quot;) == null &amp;&amp; $right.filter(&quot;Entity&quot;) == null)%>">
                  <subModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%displayWarningMessage(&quot;Binding information not created&quot;, &quot;Only DTO and Entities can be bound&quot;)%>"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Bind existing element" precondition="&lt;%filter(&quot;Category&quot;) == null%>" nodeMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']" extraMappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_ServiceDTO_CM'] //@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_ExternalServiceDTO_CM'] //@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@containerMappings[name='DBI_Entity_CM']">
            <variable name="container">
              <subVariables xsi:type="tool:AcceleoVariable" name="category" computationExpression="&lt;%$containerView.eContainer(&quot;DSemanticDiagram&quot;).target%>"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="&lt;%$category.getAllBindableElementsFor($container) - $category.getBoundElementsWithTarget($container)%>"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="secondElement" candidatesExpression="&lt;%($bindableElements + $bindableElements.ancestor()).nMinimize()%>" tree="true" rootExpression="&lt;%allRoots%>" childrenExpression="&lt;%eContents()%>" message="Choose the second bound element"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$category.getGlobalBindingRegistry()%>">
                <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%$secondElement.filter(&quot;DTO&quot;) != null || $secondElement.filter(&quot;Entity&quot;) != null%>">
                  <subModelOperations xsi:type="tool:CreateInstance" typeName="environment.BindingInfo" referenceName="bindingInfos">
                    <subModelOperations xsi:type="tool:SetValue" featureName="left" valueExpression="&lt;%$container%>"/>
                    <subModelOperations xsi:type="tool:SetValue" featureName="right" valueExpression="&lt;%$secondElement%>"/>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%$secondElement.filter(&quot;DTO&quot;) == null &amp;&amp; $secondElement.filter(&quot;Entity&quot;) == null%>">
                  <subModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%displayWarningMessage(&quot;Binding information not created&quot;, &quot;Only DTO and Entities can be bound&quot;)%>"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:OperationAction" name="Change target" precondition="&lt;%$views.nSize == 1 &amp;&amp; $views.actualMapping.name == &quot;DBI_BoundElement&quot;%>">
            <view name="views">
              <subVariables xsi:type="tool:AcceleoVariable" name="category" computationExpression="&lt;%$views.eContainer(&quot;DSemanticDiagram&quot;).target%>"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="firstElement" computationExpression="&lt;%$views.sourceNode.target.getBindingOpposite($views.targetNode.target)%>"/>
              <subVariables xsi:type="tool:AcceleoVariable" name="bindableElements" computationExpression="&lt;%$category.getAllBindableElementsFor($firstElement) - $category.getBoundElementsWithTarget($firstElement)%>"/>
              <subVariables xsi:type="tool:SelectModelElementVariable" name="secondElement" candidatesExpression="&lt;%($bindableElements + $bindableElements.ancestor()).nMinimize()%>" tree="true" rootExpression="&lt;%$category.allRoots%>" childrenExpression="&lt;%eContents()%>" message="Choose the second bound element"/>
            </view>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$views.sourceNode.target.changeBindingTarget($firstElement, $secondElement)%>"/>
            </initialOperation>
          </ownedTools>
          <subSections name="Usability">
            <ownedTools xsi:type="tool:DeleteElementDescription" name="DEL_BoundElement_PreventDeletion">
              <element name="element"/>
              <elementView name="elementView"/>
              <containerView name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%displayWarningMessage(&quot;Could not delete&quot;, &quot;Only whole binding can be deleted&quot;)%>"/>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:ReconnectEdgeDescription" name="REC_BoundElement" precondition="&lt;%$diagram.target.checkIfReconnectBindingIsPossible($element, $source, $target)%>">
              <source name="source"/>
              <target name="target"/>
              <sourceView name="sourceView"/>
              <targetView name="targetView"/>
              <element name="element"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$element.reconnectBindingTarget($source, $target)%>"/>
              </initialOperation>
              <edgeView name="edgeView"/>
            </ownedTools>
            <ownedTools xsi:type="description_2:DBindingEditorCreationDescription" name="CRE_BindingEditor" label="Binding Details" description="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='Binding%20Details']">
              <containerViewVariable name="containerView"/>
              <representationNameVariable name="name"/>
            </ownedTools>
            <ownedTools xsi:type="tool:DoubleClickDescription" name="DBL_OpenBindingEditor" mappings="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']/@defaultLayer/@nodeMappings[name='DBI_BindingInfo']">
              <element name="element"/>
              <elementView name="elementView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$element.openBindingEditor()%>"/>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:DiagramCreationDescription" name="DTO Binding" diagramDescription="//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='DTO%20Binding']">
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%current%>"/>
              </initialOperation>
              <containerViewVariable name="containerView"/>
              <representationNameVariable name="diagramName"/>
            </ownedTools>
            <ownedTools xsi:type="tool:DeleteElementDescription" name="DEL_ServiceDTO_PreventDeletion">
              <element name="element"/>
              <elementView name="elementView"/>
              <containerView name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%self%>"/>
              </initialOperation>
            </ownedTools>
          </subSections>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
