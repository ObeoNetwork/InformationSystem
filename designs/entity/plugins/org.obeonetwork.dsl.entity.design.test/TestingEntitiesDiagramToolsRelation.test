// Assert edges
with [get-editor "namespace_2 - Entities diagram" | get-diagram -index 1 | get-edit-part -name "namespace_2 - Entities diagram"] {
    get-property "edges.size" | equals 3 | verify-true
    get-property "edges.Edge[0].element.name" | equals "Entity3s [0..*]" | verify-true
    get-property "edges.Edge[1].element.name" | equals "Entity2s [0..*]" | verify-true
    get-property "edges.Edge[2].element.target.supertype.name" | equals Entity1 | verify-true
}

