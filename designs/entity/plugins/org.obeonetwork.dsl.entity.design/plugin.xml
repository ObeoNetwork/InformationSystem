<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="org.obeonetwork.dsl.entity.design.Activator"
               id="org.obeonetwork.dsl.entity.design"
	       name="entity">
    </component>
  </extension>
  <extension
        point="org.eclipse.ui.newWizards">
     <wizard
           category="org.obeonetwork.dsl.is.ui.categoryID"
           class="org.obeonetwork.dsl.entity.design.wizards.NewEntityModelWizard"
           icon="icons/full/obj16/EntityModelFile.gif"
           id="org.obeonetwork.dsl.entity.design.wizards.NewEntityModelWizard"
           name="Entity Model">
        <description>
           New entity model wizard
        </description>
     </wizard>
  </extension>
  <extension
        point="org.obeonetwork.dsl.environment.design.partialView">
     <partialView
           descriptionName="Entities Namespaces Hierarchy">
     </partialView>
     <partialView
           descriptionName="Entities Diagram">
     </partialView>
  </extension>
</plugin>
